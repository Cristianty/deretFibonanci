#include <iostream>

using namespace std;

int a, b, c;
int nilai;


int main()
{
    cout << "Program deret Fibonanci" << endl;
    cout << "Input  : " << endl;
    cin >> nilai;

    cout << "Output : " << endl;
    a=1; b=0;
    cout << a << b << endl;
    for(int i=0; i<=nilai; i++){
    c = a+b;
    a = b;
    b = c;

    cout << c << endl;
    }

    }
